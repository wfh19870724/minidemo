page {
    font-size: 28rpx;
    background: rgb(240,240,240);
    color: #333;
}

.clearfix {
    overflow: auto;
    _height: 1%;
}

.g-doctor-avatar {
    width: 100rpx;
    height: 100rpx;
    float: left;
    border-radius: 100rpx;
}

.g-doctor-info-con {
    width: 570rpx;
    float: left;
    padding-left: 20rpx;
}

.g-doctor-distance {
    width: 100rpx;
    float: left;
    padding-top: 45rpx;
    color: #999;
}

.g-score {
    display: inline;
}

.g-score.g-color-orange {
    font-weight: bolder;
}

.g-score-left {
    font-size: 34rpx;
}

.g-feature {
    height: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.g-title-size {
    font-size: 32rpx;
}

.g-middle-size {
    font-size: 28rpx;
}

.g-small-size {
    font-size: 26rpx;
}

.g-con-line {
    margin-top: 16rpx;
    line-height: 1.2;
}

.g-margin-right {
    margin-right: 20rpx;
}

.g-color-orange {
    color: #f49938;
}

.g-color-light {
    color: #999;
}

.g-tag {
    font-size: 20rpx;
    float: left;
    border: 1rpx solid #eee;
    border-radius: 20rpx;
    padding: 2rpx 10rpx;
}

.g-hospital-avatar {
    width: 145rpx;
    height: 115rpx;
    float: left;
}

.g-hospital-info-con {
    width: 425rpx;
    float: left;
    padding: 0 10rpx 0 20rpx;
}

.g-hospital-distance {
    width: 90rpx;
    float: left;
    padding-top: 45rpx;
}

.g-scroll-view-item {
    padding: 30rpx;
    border-bottom: 1rpx solid #eee;
    position: relative;
    overflow: hidden;
    background: #fff;
}

.g-list-img {
    width: 300rpx;
    height: 200rpx;
    border-radius: 6rpx;
}

.g-list-title,.g-list-line1,.g-list-line2,.g-list-line3 {
    font-size: 28rpx;
    position: absolute;
    border: none;
    left: 360rpx;
    right: 30rpx;
    color: #999;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

.g-list-title {
    font-size: 34rpx;
    top: 30rpx;
    color: #333;
}

.g-list-line1 {
    top: 100rpx;
}

.g-list-line2 {
    top: 140rpx;
}

.g-list-line3 {
    top: 180rpx;
}

.g-button {
    line-height: 60rpx;
    padding: 0 35rpx;
    text-align: center;
    background: rgb(63,134,255);
    display: inline-block;
    color: #fff;
    border-radius: 30rpx;
}

.g-hide {
    display: none;
}

.g-star {
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: cover;
    height: 30rpx;
    width: 40rpx;
    display: inline-block;
    margin-right: 10rpx;
}

.g-star.gray {
    background-image: url("data:image/png;base64,");
}

.g-arrow-right {
    position: relative;
}

.g-arrow-right::after {
    content: "";
    display: block;
    position: absolute;
    right: 25rpx;
    top: 50%;
    margin-top: -12rpx;
    width: 18rpx;
    height: 18rpx;
    border-right: 4rpx solid rgb(216,216,216);
    border-bottom: 4rpx solid rgb(216,216,216);
    transform: rotate(-45deg);
}

.g-loading-more {
    background: #fff;
    text-align: center;
    line-height: 100rpx;
    color: rgb(67,86,132);
}

.g-empty {
    width: 750rpx;
    text-align: center;
    background: #fff;
    padding-top: 100rpx;
}

.g-empty-img {
    width: 231rpx;
    height: 230rpx;
}

@-webkit-keyframes dot1 {
    0% {
        transform: rotateZ(30deg);
    }

    95%,100% {
        transform: rotateZ(390deg);
    }
}

@keyframes dot1 {
    0% {
        transform: rotateZ(30deg);
    }

    95%,100% {
        transform: rotateZ(390deg);
    }
}

@-webkit-keyframes dot2 {
    0% {
        transform: rotateZ(15deg);
    }

    95%,100% {
        transform: rotateZ(375deg);
    }
}

@keyframes dot2 {
    0% {
        transform: rotateZ(15deg);
    }

    95%,100% {
        transform: rotateZ(375deg);
    }
}

@-webkit-keyframes dot3 {
    0% {
        transform: rotateZ(0deg);
    }

    95%,100% {
        transform: rotateZ(360deg);
    }
}

@keyframes dot3 {
    0% {
        transform: rotateZ(0deg);
    }

    95%,100% {
        transform: rotateZ(360deg);
    }
}

@-webkit-keyframes dot4 {
    0% {
        transform: rotateZ(-15deg);
    }

    95%,100% {
        transform: rotateZ(345deg);
    }
}

@keyframes dot4 {
    0% {
        transform: rotateZ(-15deg);
    }

    95%,100% {
        transform: rotateZ(345deg);
    }
}

.g-loading {
    height: 48px;
    width: 48px;
    position: relative;
    margin: 15px auto;
}

.g-dot {
    position: absolute;
    width: 8px;
    height: 8px;
    left: 50%;
    top: 0;
    margin-left: -5px;
    border-radius: 50%;
    background-color: #ddd;
    transform-origin: 50% 24px;
}

.g-dot:nth-child(1) {
    animation: dot1 1.5s cubic-bezier(.6,0,.4,1) 0ms infinite;
}

.g-dot:nth-child(2) {
    animation: dot2 1.5s cubic-bezier(.6,0,.4,1) 100ms infinite;
    width: 7px;
    height: 7px;
}

.g-dot:nth-child(3) {
    animation: dot3 1.5s cubic-bezier(.6,0,.4,1) 200ms infinite;
    width: 6px;
    height: 6px;
}

.g-dot:nth-child(4) {
    animation: dot4 1.5s cubic-bezier(.6,0,.4,1) 300ms infinite;
    width: 5px;
    height: 5px;
}

.g-hide {
    display: none;
}

.wy-btn {
    background-color: #3f86ff;
    color: #fff;
    height: 80rpx;
    line-height: 80rpx;
    display: inline-block;
    border-radius: 50rpx;
    font-weight: normal;
    font-size: 26rpx;
    padding: 0 70rpx;
}

.g-close {
    position: absolute;
    right: 25rpx;
    top: 25rpx;
    height: 50rpx;
    width: 50rpx;
}

.g-close:after,.g-close:before {
    width: 4rpx;
    height: 30rpx;
    background: rgb(220,220,220);
    content: "";
    display: block;
    position: absolute;
    right: 10rpx;
    top: 0rpx;
    z-index: 1;
}

.g-close:after {
    transform: rotate(45deg);
}

.g-close:before {
    transform: rotate(-45deg);
}