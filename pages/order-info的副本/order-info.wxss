page {
    padding-bottom: 150rpx;
}

.order-card {
    background: #fff;
}

.doctor {
    line-height: 100rpx;
    border-top: 1rpx solid rgb(238,238,238);
    border-bottom: 1rpx solid rgb(238,238,238);
    color: rgb(153,153,153);
}

.doctor image {
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    display: inline-block;
    vertical-align: middle;
    margin: 0 20rpx;
}

.doctor text {
    color: rgb(51,51,51);
    padding-right: 20rpx;
}

.orange {
    color: rgb(246,127,59);
}

.order-info {
    border-bottom: 1rpx solid rgb(238,238,238);
    padding: 30rpx 25rpx;
}

.line {
    line-height: 50rpx;
}

textarea {
    padding-bottom: 40rpx;
}

.line .key {
    color: rgb(102,102,102);
    padding-right: 65rpx;
    float: left;
}

.order-form {
    margin: 20rpx 0;
    background: #fff;
}

.order-form .line {
    line-height: 100rpx;
    padding: 0 25rpx;
    border-bottom: 1rpx solid rgb(238,238,238);
    overflow: hidden;
}

.order-form .line .value {
    float: right;
    margin-right: 30rpx;
}

.order-form .card {
    line-height: 100rpx;
    height: 100rpx;
    text-align: right;
}

input.card {
    width: 200rpx;
}

.gray {
    color: rgb(130,130,130);
}

.rule {
    line-height: 40rpx;
    padding: 10rpx 25rpx;
    margin-top: 25rpx;
    background: #fff;
    border-top: 1rpx solid rgb(238,238,238);
}

.rule.g-arrow-right:after {
}

.rule text {
    display: block;
}

.rule-detail {
    line-height: 50rpx;
    font-size: 24rpx;
    padding: 0 25rpx;
    background: #fff;
}

.next {
    color: #fff;
    line-height: 100rpx;
    background: rgb(63,134,255);
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    text-align: center;
    z-index: 1;
    border-radius: 0;
    font-size: 28rpx;
}

.modal-group {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.3);
    z-index: 2;
}

.content {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgb(248,248,248);
}

.chose-time .title {
    line-height: 90rpx;
    text-align: center;
    display: block;
}

.chose-time .list {
    height: 500rpx;
    margin-bottom: 100rpx;
    background: rgb(248,248,248);
    padding: 0 25rpx;
}

.chose-time .item {
    width: 700rpx;
    line-height: 90rpx;
    text-align: center;
    background: #fff;
    margin-bottom: 10rpx;
    border-radius: 60rpx;
    border: 1rpx solid rgb(238,238,238);
}

.item.gray {
    color: #ccc;
}

.chose-time .item.active {
    border: 1rpx solid rgb(63,134,255);
    color: rgb(63,134,255);
}

.chose-time .item.positive {
    color: rgb(153,153,153);
}

.chose-time .cancel {
    line-height: 100rpx;
    text-align: center;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    background: #fff;
    border-top: 1rpx solid rgb(238,238,238);
}

.chose-disease,.chose-patient,.add-patient {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 800rpx;
    background: #fff;
    border-radius: 20rpx 20rpx 0 0;
    padding: 25rpx 0;
}

.chose-disease .title {
    text-align: center;
    line-height: 60rpx;
}

.chose-disease .search {
    width: 700rpx;
    height: 60rpx;
    margin: 0 auto;
    background: rgb(248,248,248);
    border-radius: 50rpx;
    position: relative;
}

.chose-disease .search input {
    height: 60rpx;
    line-height: 60rpx;
    margin-left: 60rpx;
    width: 600rpx;
}

.chose-disease .search-list {
    width: 750rpx;
    height: 630rpx;
    margin-top: 20rpx;
}

.chose-disease .search-item {
    line-height: 80rpx;
    padding-left: 40rpx;
    border-top: 1rpx solid rgb(247,247,247);
    color: rgb(96,96,96);
}

.chose-disease .search-item:last-child {
    border-bottom: 1rpx solid rgb(247,247,247);
}

.chose-disease .search:before {
    background-image: url("data:image/png;base64,");
    background-size: cover;
    content: '';
    display: block;
    position: absolute;
    left: 12rpx;
    top: 9rpx;
    width: 40rpx;
    height: 40rpx;
}

.chose-patient .title {
    text-align: center;
    line-height: 60rpx;
}

.chose-patient .sub-title {
    text-align: center;
    color: rgb(168,168,168);
    font-size: 24rpx;
}

.chose-patient .patient-list {
    margin-top: 25rpx;
    height: 580rpx;
}

.patient {
    padding: 25rpx;
    border-top: 1rpx solid rgb(247,247,247);
    position: relative;
}

.patient:last-child {
    border-bottom: 1rpx solid rgb(247,247,247);
}

.patient .icon {
    content: "";
    display: block;
    width: 40rpx;
    height: 40rpx;
    background: url("data:image/png;base64,") center center no-repeat / 39rpx 39rpx;
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
}

.block {
    display: block;
}

.patient text {
    line-height: 50rpx;
    margin-right: 20rpx;
}

.patient.inline text {
    display: inline-block;
}

.patient .name {
    font-size: 32rpx;
    color: #333;
}

.add-btn {
    width: 700rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin: 20rpx auto;
    text-align: center;
    color: #fff;
    background: rgb(63,134,255);
    border-radius: 50rpx;
    font-size: 28rpx;
}

.add-patient {
    height: 850rpx;
}

.add-patient.edit {
    height: 610rpx;
}

.add-patient .title {
    text-align: center;
    line-height: 60rpx;
}

.add-patient .warning {
    background: rgb(244,56,37);
    line-height: 60rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
}

.add-patient .line {
    line-height: 100rpx;
    border-top: 1rpx solid rgb(247,247,247);
    padding-left: 25rpx;
}

.add-patient .line:last-child {
    border-bottom: 1rpx solid rgb(247,247,247);
}

.add-patient .line .value {
    float: right;
    height: 100rpx;
    line-height: 100rpx;
    width: 550rpx;
    color: rgb(128,128,128);
}

.add-patient .switch {
    float: right;
    margin-right: 20rpx;
}

.error {
    color: rgb(244,0,0)!important;
}

.sex label {
    width: 130rpx;
    height: 50rpx;
    line-height: 50rpx;
    display: inline-block;
    text-align: center;
    border: 1rpx solid rgb(238,238,238);
    margin-right: 20rpx;
    border-radius: 40rpx;
    color: rgb(128,128,128);
}

.sex radio {
    display: none;
    opacity: 0;
}

.sex label.active {
    color: rgb(92,145,255);
    border-color: rgb(92,145,255);
}

.valid-code {
    width: 560rpx;
    height: 360rpx;
    background: #fff;
    position: absolute;
    top: 40%;
    left: 50%;
    margin-left: -280rpx;
    margin-top: -180rpx;
    border-radius: 30rpx;
    padding-top: 30rpx;
}

.valid-code .title {
    line-height: 60rpx;
    text-align: center;
}

.valid-code .warning {
    background: rgb(244,56,37);
    line-height: 50rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
}

.valid-code .phone {
    line-height: 75rpx;
    padding: 0 0 0 40rpx;
    margin-top: 10rpx;
}

.valid-code .resend {
    display: inline-block;
    margin-left: 30rpx;
    width: 150rpx;
    height: 50rpx;
    line-height: 50rpx;
    border: 1rpx solid rgb(219,219,219);
    border-radius: 40rpx;
    text-align: center;
    color: rgb(102,102,102);
}

.valid-code .code-group {
    overflow: hidden;
    border: 1rpx solid rgb(219,219,219);
    width: 481rpx;
    margin: 30rpx auto 0 auto;
    display: flex;
    border-radius: 6rpx;
}

.valid-code .code {
    flex: 1;
    line-height: 80rpx;
    height: 80rpx;
    float: left;
    border-right: 1rpx solid rgb(219,219,219);
    text-align: center;
}

.valid-code .code:last-child {
    border-right: none;
}

.hidden-input {
    position: absolute;
    width: 480rpx;
    left: 36rpx;
    bottom: 58rpx;
    height: 80rpx;
    opacity: 0;
    display: none;
}