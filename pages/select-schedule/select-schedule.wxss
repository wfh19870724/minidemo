page {
    background: #f8f8f8;
}

.doctor-info-wrap {
    width: 750rpx;
    height: 210rpx;
    background: url("data:image/png;base64,");
    background-position: center bottom;
    background-size: cover;
    padding: 30rpx;
    box-sizing: border-box;
    position: relative;
}

.doctor-avatar {
    width: 130rpx;
    height: 130rpx;
    border-radius: 50%;
    border: 6rpx solid rgba(255,255,255,0.3);
    float: left;
}

.doctor-info-wrap text {
    line-height: 50rpx;
    text-indent: 30rpx;
    color: rgba(255,255,255,0.8);
    display: block;
}

.doctor-info-wrap .doctor-name {
    font-size: 36rpx;
    color: #fff;
}

.doctor-title {
    font-size: 24rpx;
    padding-top: 5rpx;
}

.doctor-hospital {
    font-size: 24rpx;
}

.doctor-info-wrap .doctor-order {
    font-size: 24rpx;
    width: 220rpx;
    position: absolute;
    right: 25rpx;
    top: 45rpx;
    text-align: center;
}

.doctor-order text {
    color: rgb(249,212,13);
    font-size: 32rpx;
    display: inline;
    vertical-align: middle;
    padding-left: 10rpx;
}

.sub-line {
    line-height: 80rpx;
    width: 750rpx;
    background: #fff;
    border-bottom: 1rpx solid rgb(242,242,242);
    text-indent: 25rpx;
}

.sub-line .link {
    float: right;
    padding-right: 60rpx;
}

.main-line {
    line-height: 100rpx;
    width: 750rpx;
    background: #fff;
    border-bottom: 1rpx solid rgb(242,242,242);
    text-indent: 25rpx;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    padding-right: 100rpx;
    box-sizing: border-box;
    position: relative;
    padding-left: 30rpx;
}

.main-line:before {
    content: '';
    position: absolute;
    top: 45rpx;
    left: 20rpx;
    width: 10rpx;
    height: 10rpx;
    background-color: #3278ee;
    border-radius: 4rpx;
}

.g-arrow-right.arrow-top:after {
    transform: rotate(-135deg);
}

.g-arrow-right.arrow-bottom:after {
    transform: rotate(45deg);
}

.schedule-item {
    width: 100%;
    color: #666;
    font-size: 24rpx;
}

.month-item {
    height: 72rpx;
    line-height: 72rpx;
    position: relative;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    padding-left: 24rpx;
}

.shift-item {
    height: 120rpx;
    margin: 0 24rpx;
    border-bottom: 1rpx solid #EBECF1;
    position: relative;
    overflow: hidden;
}

.bookDate {
    font-size: 28rpx;
    color: #28354C;
    line-height: 40rpx;
    margin-top: 20rpx;
}

.dayDistance {
    border: 1rpx solid #3F86FF;
    border-radius: 200rpx;
    border-width: 2rpx!important;
    font-size: 24rpx;
    color: #3F86FF;
    font-style: normal;
    padding: 0 12rpx;
    text-align: center;
    display: inline-block;
    height: 28rpx;
    line-height: 28rpx;
}

.platform {
    font-size: 24rpx;
    color: #83889A;
    line-height: 40rpx;
}

.price {
    color: #ff695c;
    margin-left: 10rpx;
}

.btn-book {
    border-radius: 200rpx;
    height: 60rpx;
    width: 120rpx;
    display: inline-block;
    position: absolute;
    top: 50%;
    right: 0;
    transform: translate(0,-30rpx);
    font-size: 28rpx;
    color: #FFF;
    line-height: 60rpx;
    text-align: center;
}

.blue {
    background: #3F86FF;
}

.gray {
    background: #C4C5CE;
}

.calendar {
    width: 700rpx;
    float: left;
    background: #fff;
    white-space: nowrap;
}

.sub-line.show-all {
    display: inline-block;
    border-bottom: none;
    text-align: center;
    text-indent: 0;
}

.color-gray {
    color: rgb(153,153,153);
}

.modal-group {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0,0,0,0.3);
    z-index: 2;
}

.content {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgb(248,248,248);
}

.add-patient {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 800rpx;
    background: #fff;
    border-radius: 20rpx 20rpx 0 0;
    padding-top: 25rpx;
}

.add-btn {
    width: 700rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin: 20rpx auto;
    text-align: center;
    color: #fff;
    background: rgb(63,134,255);
    border-radius: 50rpx;
    font-size: 28rpx;
}

.add-patient {
    height: 850rpx;
}

.add-patient.edit {
    height: 550rpx;
}

.add-patient .title {
    text-align: center;
    line-height: 60rpx;
}

.add-patient .warning {
    background: rgb(244,56,37);
    line-height: 60rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
}

.add-patient .line {
    line-height: 100rpx;
    border-top: 1rpx solid rgb(247,247,247);
    padding-left: 25rpx;
}

.add-patient .line:last-child {
    border-bottom: 1rpx solid rgb(247,247,247);
}

.add-patient .line .value {
    float: right;
    height: 100rpx;
    line-height: 100rpx;
    width: 550rpx;
    color: rgb(128,128,128);
}

.add-patient .switch {
    float: right;
    margin-right: 20rpx;
}

.error {
    color: rgb(244,0,0)!important;
}